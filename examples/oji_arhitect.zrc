#!../bin/zrc
#This is an interesting problem from an informatics olympiad
#(I/O and file demo)
set max = -1

fn fin {
	let i {
	for {set i = 1} {$i < $argc} {inc i} {
		set $argv($i) = $(read str)
	}}
}

fn main {
	set n = $(read str)
	while {$n>0} {
		fin x1 y1 x2 y2
		set num = [expr abs(($x1-$x2)/($y1-$y2))]
		if {![string cmp $hm($num) '']} {
			set hm($num) = 0
		}
		inc hm($num)
		if {$hm($num) > $max} {
			set max = $hm($num)
		}
		inc n -1
	}
	echo $max
};@ main
